<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ProfileEditTextWidget">
        <attr name="hint" format="string"/>
        <attr name="editable" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="AvatarWidget">
        <attr name="type" format="enum">
            <enum name="mini" value="0"/>
            <enum name="normal" value="1"/>
        </attr>
        <attr name="bg" format="enum">
            <enum name="employee" value="0"/>
            <enum name="client" value="1"/>
        </attr>
    </declare-styleable>
</resources>